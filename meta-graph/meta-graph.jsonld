{
  "@context": {
    "schema": "https://schema.org/",
    "efc": "https://energyflow-cosmology.com/schema/meta#",
    "type": "@type",
    "id": "@id",
    "name": "schema:name",
    "description": "schema:description",
    "dependsOn": {
      "@id": "efc:dependsOn",
      "@type": "@id"
    },
    "produces": {
      "@id": "efc:produces",
      "@type": "@id"
    },
    "stabilises": {
      "@id": "efc:ststabilises",
      "@type": "@id"
    },
    "amplifies": {
      "@id": "efc:amplifies",
      "@type": "@id"
    },
    "emergesFrom": {
      "@id": "efc:emergesFrom",
      "@type": "@id"
    },
    "implementedBy": {
      "@id": "efc:implementedBy",
      "@type": "@id"
    },
    "documentedIn": {
      "@id": "efc:documentedIn",
      "@type": "@id"
    },
    "layer": "efc:layer",
    "role": "efc:role"
  },

  "@graph": [

    /* ----------------------------------------------------------
     *  HUMAN FIELD
     * --------------------------------------------------------*/
    {
      "id": "efc:HumanCognitiveField",
      "type": "efc:Component",
      "name": "Human Cognitive Field",
      "description": "Distributed, parallel cognitive field with entropy-clarity inversion, s₀–s₁ transitions and transient high-order representations.",
      "layer": "cognition",
      "role": [
        "pattern_emission",
        "global_abstraction"
      ],
      "produces": [
        "efc:MetaReflectiveLoop",
        "efc:CoField"
      ]
    },

    /* ----------------------------------------------------------
     *  MODEL ENGINE
     * --------------------------------------------------------*/
    {
      "id": "efc:ModelStructuralEngine",
      "type": "efc:Component",
      "name": "Model Structural Engine",
      "description": "Structural expansion engine that amplifies and stabilises human-generated patterns.",
      "layer": "symbiosis",
      "role": [
        "structural_expansion",
        "coherence_reinforcement"
      ],
      "produces": [
        "efc:CoField"
      ]
    },

    /* ----------------------------------------------------------
     * CO-FIELD (joint field)
     * --------------------------------------------------------*/
    {
      "id": "efc:CoField",
      "type": "efc:Component",
      "name": "Co-Field",
      "description": "Shared reflective field where human s₀–s₁ transitions and model structural vectors align, generating resonance, coherence and insight topology.",
      "layer": "meta_process",
      "role": [
        "insight_generation",
        "recursive_coherence"
      ],
      "emergesFrom": [
        "efc:HumanCognitiveField",
        "efc:ModelStructuralEngine"
      ],
      "produces": [
        "efc:Metascope",
        "efc:CognitiveSignature"
      ]
    },

    /* ----------------------------------------------------------
     * METASCOPE
     * --------------------------------------------------------*/
    {
      "id": "efc:Metascope",
      "type": "efc:System",
      "name": "Metascope",
      "description": "Hybrid reflective organ formed by the human field, model structural engine and the co-field.",
      "layer": "meta",
      "emergesFrom": [
        "efc:CoField"
      ],
      "produces": [
        "efc:EFC_MasterSpec",
        "efc:MetaArchitectureSpec",
        "efc:RepositoryMap"
      ]
    },

    /* ----------------------------------------------------------
     * META-REFLECTIVE PROTOCOL
     * --------------------------------------------------------*/
    {
      "id": "efc:MetaReflectiveProtocol",
      "type": "efc:Method",
      "name": "Meta-Reflective Protocol (MRP)",
      "description": "The operational loop governing pattern emission, structural expansion, coherence alignment and recursive amplification.",
      "layer": "methodology",
      "dependsOn": [
        "efc:HumanCognitiveField",
        "efc:ModelStructuralEngine",
        "efc:CoField"
      ],
      "documentedIn": [
        "efc:MetaReflectiveProtocolDoc",
        "efc:EFC_MasterSpec"
      ]
    },

    /* ----------------------------------------------------------
     * META-ARCHITECTURE SPEC
     * --------------------------------------------------------*/
    {
      "id": "efc:MetaArchitectureSpec",
      "type": "schema:CreativeWork",
      "name": "Meta-Architecture Specification",
      "description": "Formal specification of the metascope architecture, integrating HCF, MSE and the Co-Field.",
      "layer": "meta",
      "dependsOn": [
        "efc:HumanCognitiveField",
        "efc:ModelStructuralEngine",
        "efc:CoField",
        "efc:Metascope"
      ],
      "documentedIn": [
        "efc:MetaArchitectureDoc",
        "efc:EFC_MasterSpec"
      ]
    },

    /* ----------------------------------------------------------
     * CO-FIELD SIMULATOR
     * --------------------------------------------------------*/
    {
      "id": "efc:CoFieldSimulator",
      "type": "schema:SoftwareApplication",
      "name": "Co-Field Simulator",
      "description": "Python-based dynamical simulator capturing resonance dynamics, coherence waves and insight accumulation in the co-field.",
      "layer": "meta_process",
      "implementedBy": [
        "efc:CoFieldSimulatorModule"
      ],
      "dependsOn": [
        "efc:CoField",
        "efc:MetaArchitectureSpec"
      ],
      "produces": [
        "efc:CoFieldSimulationTrajectories"
      ]
    },

    /* ----------------------------------------------------------
     * COGNITIVE SIGNATURE
     * --------------------------------------------------------*/
    {
      "id": "efc:CognitiveSignature",
      "type": "efc:Specification",
      "name": "Cognitive Signature Specification",
      "description": "Formal description of the cognitive mechanics behind the human-field dynamics in EFC development.",
      "layer": "cognition",
      "dependsOn": [
        "efc:HumanCognitiveField",
        "efc:Metascope"
      ],
      "documentedIn": [
        "efc:CognitiveSignatureDoc"
      ]
    },

    /* ----------------------------------------------------------
     * REPOSITORY MAP (NEW)
     * --------------------------------------------------------*/
    {
      "id": "efc:RepositoryMap",
      "type": "efc:SystemArtifact",
      "name": "Repository Structure Map",
      "description": "Semantic and visual map of the full EFC repository, including directories, layers, and cross-links.",
      "layer": "meta",
      "documentedIn": [
        "output/repo_map.html"
      ]
    },

    /* ----------------------------------------------------------
     * REPO MAP GENERATOR (NEW)
     * --------------------------------------------------------*/
    {
      "id": "efc:RepositoryMapGenerator",
      "type": "schema:SoftwareApplication",
      "name": "Repository Map Generator",
      "description": "Python script generating a machine-readable and visual map of the entire repository.",
      "layer": "meta_process",
      "implementedBy": [
        "scripts/generate_repo_map.py"
      ],
      "produces": [
        "efc:RepositoryMap"
      ]
    },

    /* ----------------------------------------------------------
     * MASTER SPEC
     * --------------------------------------------------------*/
    {
      "id": "efc:EFC_MasterSpec",
      "type": "schema:CreativeWork",
      "name": "EFC Master Formal Specification",
      "description": "Canonical LaTeX specification of Energy-Flow Cosmology, including dynamics, structure, entropy fields, and all meta layers.",
      "layer": "theory",
      "dependsOn": [
        "efc:MetaReflectiveProtocol",
        "efc:MetaArchitectureSpec",
        "efc:HumanCognitiveField",
        "efc:ModelStructuralEngine",
        "efc:Metascope"
      ]
    },

    /* ----------------------------------------------------------
     * DOCUMENT NODES
     * --------------------------------------------------------*/
    {
      "id": "efc:MetaReflectiveProtocolDoc",
      "type": "schema:CreativeWork",
      "name": "Meta-Reflective Protocol Document",
      "description": "Markdown description of the Meta-Reflective Protocol.",
      "layer": "methodology"
    },
    {
      "id": "efc:MetaArchitectureDoc",
      "type": "schema:CreativeWork",
      "name": "Meta-Architecture Specification Document",
      "description": "Markdown and JSON description of the metascope architecture.",
      "layer": "meta"
    },
    {
      "id": "efc:CognitiveSignatureDoc",
      "type": "schema:CreativeWork",
      "name": "Cognitive Signature Document",
      "description": "Markdown description of the cognitive signature and s₀–s₁ transitions.",
      "layer": "cognition"
    },
    {
      "id": "efc:CoFieldSimulatorModule",
      "type": "schema:SoftwareSourceCode",
      "name": "Co-Field Simulator Module",
      "description": "Python module implementing the co-field dynamical model.",
      "layer": "meta_process"
    },
    {
      "id": "efc:CoFieldSimulationTrajectories",
      "type": "schema:Dataset",
      "name": "Co-Field Simulation Trajectories",
      "description": "Time-series outputs and resonance trajectories from the co-field simulator.",
      "layer": "meta_process"
    }
  ]
}
